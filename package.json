    "pg": "^7.11.0",
    "pg-hstore": "^2.3.3",
    "postcss": "^7.0.17",
    "postcss-cli": "^6.1.2",
    "pug": "^2.0.3",
    "sequelize": "^5.8.7"
  },
  "browserslist": [
    "last 2 versions",
    "not ie < 11",
    "Android >= 6",
    "iOS >= 10"
